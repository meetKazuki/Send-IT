  "scripts": {
    "build": "babel api/server.js --out-dir build",
    "dev-start": "babel-watch api/server.js",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha api/test/test.js"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetKazuki/Send-IT.git"
  },
  "author": "Desmond Edem <kukiito219@gmail.com> (https://meetkazuki.github.io)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meetKazuki/Send-IT/issues"
  },
  "homepage": "https://github.com/meetKazuki/Send-IT#readme",
  "dependencies": {
    "express": "^4.16.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
